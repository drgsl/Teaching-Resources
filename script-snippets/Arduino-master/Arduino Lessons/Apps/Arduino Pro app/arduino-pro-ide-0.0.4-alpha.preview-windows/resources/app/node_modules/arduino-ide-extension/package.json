  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/chai-string": "^1.4.2",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "decompress": "^4.2.0",
    "decompress-targz": "^4.1.1",
    "decompress-unzip": "^4.0.1",
    "download": "^7.1.0",
    "grpc-tools": "^1.8.0",
    "grpc_tools_node_protoc_ts": "^2.5.8",
    "mocha": "^7.0.0",
    "moment": "^2.24.0",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.1",
    "shelljs": "^0.8.3",
    "ts-node": "^8.6.2",
    "tslint": "^5.5.0",
    "typescript": "3.5.3",
    "uuid": "^3.2.1",
    "yargs": "^11.1.0"
  },
  "files": [
    "lib",
    "src",
    "build",
    "data"
